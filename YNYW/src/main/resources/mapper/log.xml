<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" > 
<mapper namespace="com.example.demo.dao.LogDao"> 

 	<insert id="AddLog" parameterType="com.example.demo.Entity.userRecordtime">
 			insert into t_log(user_id,register_datetime)
	        values(#{userId},#{registerDatetime}) 
 	</insert>
 	<select id="logs" resultType="com.example.demo.Entity.logs">
 	select t_log.user_id,user_name,authority,register_datetime from t_user join t_log on t_user.user_id=t_log.user_id
 	</select>

</mapper>